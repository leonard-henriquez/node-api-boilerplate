{
  "env": {
    "node": true,
    "commonjs": true,
    "jest": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "consistent-return": 0,
    "func-names": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "semi": ["error", "never"]
  }
}
