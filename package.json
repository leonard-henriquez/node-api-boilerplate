{
  "name": "node-api-boilerplate",
  "version": "1.0.0",
  "description": "Node API Boilerplate",
  "author": "Leonard Henriquez <leonard.henriquez@edhec.com>",
  "main": "src/server.js",
  "private": false,
  "scripts": {
    "lint": "./node_modules/.bin/eslint -c .eslintrc.json src",
    "lint:fix": "./node_modules/.bin/eslint --fix -c .eslintrc.json src",
    "test": "./node_modules/.bin/jest -c jest.config.js",
    "test:watch": "./node_modules/.bin/jest --watch -c jest.config.js",
    "test:coverage": "./node_modules/.bin/jest --coverage -c jest.config.js --detectOpenHandles",
    "test:coveralls": "./node_modules/.bin/jest --coverage -c jest.config.js --coverageReporters=text-lcov | coveralls",
    "routes": "./bin/routes",
    "start": "./node_modules/.bin/nodemon --exec ./node_modules/.bin/babel-node src/server.js",
    "build": "./node_modules/.bin/babel src --out-dir dist",
    "serve": "NODE_ENV=production LOG_LEVEL=error node dist/server.js"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "convict": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-pino-logger": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.6.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pino": "^5.12.6",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/runtime": "^7.5.0",
    "babel-jest": "^24.8.0",
    "coveralls": "^3.0.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jest": "^24.8.0",
    "nodemon": "^1.18.10",
    "pino-pretty": "^3.2.0",
    "supertest": "^4.0.2"
  },
  "browserslist": [
    "node 10.16.0"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:leonard-henriquez/node-api-boilerplate.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "jest",
    "es6",
    "REST",
    "API",
    "JsonWebToken",
    "jwt",
    "authentication",
    "boilerplate"
  ]
}
