{
  "name": "node-api-boilerplate",
  "version": "1.0.0",
  "description": "Node API Boilerplate",
  "main": "src/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint -c .eslintrc.json src",
    "fix": "./node_modules/.bin/eslint --fix -c .eslintrc.json src",
    "test": "./node_modules/.bin/jest -c jest.config.js",
    "test:watch": "./node_modules/.bin/jest --watch -c jest.config.js",
    "test:coverage": "./node_modules/.bin/jest --coverage -c jest.config.js",
    "start": "./node_modules/.bin/nodemon --exec babel-node src/server.js",
    "build": "./node_modules/.bin/babel src --out-dir dist",
    "serve": "NODE_ENV=production LOG_LEVEL=error node ./dist/server.js"
  },
  "engines": {
    "node": "^10.16.0"
  },
  "author": "Leonard Henriquez",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "convict": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.6.3",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "supertest": "^4.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.5.0",
    "@babel/runtime": "^7.4.3",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jest": "^24.8.0",
    "nodemon": "^1.18.10",
    "supertest": "^4.0.2"
  }
}
